{
  "NgBootstrap Accordion": {
    "prefix": "ngb-accordion",
    "body": [
      "<ngb-accordion #acc=\"ngbAccordion\" activeIds=\"ngb-panel-0\">\n\t<ngb-panel title=\"${1:Title1}\">\n\t\t<template ngbPanelContent>\n\t\t\t${2:Content1}\n\t\t</template>\n\t</ngb-panel>\n\t<ngb-panel title=\"${3:Title2}\">\n\t\t<template ngbPanelContent>\n\t\t\t${4:Content2}\n\t\t</template>\n\t</ngb-panel>\n</ngb-accordion>$5"
    ],
    "description": "NgBootstrap Accordion"
  },
  "NgBootstrap Alert": {
    "prefix": "ngb-alert",
    "body": "<ngb-alert [type]=\"${1:default|primary|success|info|warning|danger}\" [dismissible]=\"${2:true}\">${3:Alert}</ngb-alert>$4",
    "description": "NgBootstrap Alert"
  },
  "NgBootstrap Button": {
    "prefix": "ngb-btn",
    "body": "<button type=\"button\" class=\"btn btn-${1:default|primary|success|info|warning|danger}\" (click)=\"${2:onClick}(${3:$event})\">\n\t${4:Button}\n</button>$5",
    "description": "NgBootstrap Button"
  },
  "NgBootstrap Button Outline": {
    "prefix": "ngb-btn-outline",
    "body": "<button type=\"button\" class=\"btn btn-outline-${1:default|primary|success|info|warning|danger}\" (click)=\"${2:onClick}(${3:$event})\">\n\t${4:Button}\n</button>$5",
    "description": "NgBootstrap Button Outline"
  },
  "NgBootstrap Raidio buttons group": {
    "prefix": "ngb-radio-group",
    "body": "<div ngbRadioGroup name=\"${1:radioGroup1}\">\n\t<label class=\"btn btn-${2:default|primary|success|info|warning|danger}\">\n\t\t<input type=\"radio\" [(ngModel)]=\"${3:modal.first}\"> ${4:First}\n\t</label>\n\t<label class=\"btn btn-${5:default|primary|success|info|warning|danger}\">\n\t\t<input type=\"radio\" [(ngModel)]=\"${6:modal.second}\"> ${7:Second}\n\t</label>\n</div>$8",
    "description": "NgBootstrap Raidio buttons group"
  },
  "NgBootstrap Raidio button": {
    "prefix": "ngb-radio-btn",
    "body": "<label class=\"btn btn-${1:default|primary|success|info|warning|danger}\">\n\t<input type=\"radio\" [(ngModel)]=\"${2:modal.first}\"> ${3:Label}\n</label>$4",
    "description": "NgBootstrap Raidio button"
  },
  "NgBootstrap Checkbox buttons group": {
    "prefix": "ngb-checkbox-group",
    "body": "<div class=\"btn-group\" data-toggle=\"buttons\">\n\t<label class=\"btn btn-${1:default|primary|success|info|warning|danger}\" [class.active]=\"${2:model.first}\">\n\t\t<input type=\"checkbox\" [(ngModel)]=\"${2:model.first}\">${3:First}\n\t</label>\n\t<label class=\"btn btn-${4:default|primary|success|info|warning|danger}\" [class.active]=\"${5:model.first}\">\n\t\t<input type=\"checkbox\" [(ngModel)]=\"${5:model.first}\">${6:Second}\n\t</label>\n</div>$7",
    "description": "NgBootstrap Checkbox buttons group"
  },
  "NgBootstrap Checkbox button": {
    "prefix": "ngb-checkbox-btn",
    "body": "<label class=\"btn btn-${1:default|primary|success|info|warning|danger}\" [class.active]=\"${2:model.first}\">\n\t<input type=\"checkbox\" [(ngModel)]=\"${2:model.first}\">${3:Label}\n</label>$4",
    "description": "NgBootstrap Raidio buttons"
  },
  "NgBootstrap Collapse": {
    "prefix": "ngb-collapse",
    "body": "<button type=\"button\" class=\"btn btn-${1:default|primary|success|info|warning|danger}\" (click)=\"isCollapsed = !isCollapsed\">${2:Toggle}</button>\n<div [ngbCollapse]=\"isCollapsed\">\n\t${3:Content}\n</div>$4",
    "description": "NgBootstrap Collapse"
  },
  "NgBootstrap Datepicker": {
    "prefix": "ngb-datepicker",
    "body": "<ngb-datepicker #${1:dp} [(ngModel)]=\"${2:model}\" (navigate)=\"date = $event.next\"></ngb-datepicker>$3",
    "description": "NgBootstrap Datepicker"
  },
  "NgBootstrap Datepicker Popup": {
    "prefix": "ngb-datepicker-popup",
    "body": "<div class=\"input-group\">\n\t<input class=\"form-control\" placeholder=\"${1:yyyy-mm-dd}\" name=\"${2:dp}\" [(ngModel)]=\"${3:model}\" ngbDatepicker #${4:d}=\"ngbDatepicker\">\n\t<div class=\"input-group-addon\" (click)=\"${4:d}.toggle()\">\n\t\t<img src=\"img/calendar-icon.svg\" style=\"width: 1.2rem; height: 1rem; cursor: pointer;\"/>\n\t</div>\n</div>$5",
    "description": "NgBootstrap Datepicker Popup"
  },
  "NgBootstrap Dropdown": {
    "prefix": "ngb-dropdown",
    "body": "<div ngbDropdown class=\"d-inline-block\">\n\t<button class=\"btn btn-${1:default|primary|success|info|warning|danger}\" id=\"${2:dropdownMenu1}\" ngbDropdownToggle>${3:Toggle dropdown}</button>\n\t<div class=\"dropdown-menu\" aria-labelledby=\"${2:dropdownMenu1}\">\n\t\t<button class=\"dropdown-item\">${4:Action - 1}</button>\n\t\t<button class=\"dropdown-item\">${5:Action - 2}</button>\n\t</div>\n</div>$6",
    "description": "NgBootstrap Dropdown"
  },
  "NgBootstrap Modal": {
    "prefix": "ngb-modal",
    "body": "<template #content let-c=\"close\" let-d=\"dismiss\">\n\t<div class=\"modal-header\">\n\t\t<button type=\"button\" class=\"close\" aria-label=\"Close\" (click)=\"d('Cross click')\">\n\t\t\t<span aria-hidden=\"true\">&times;</span>\n\t\t</button>\n\t<h4 class=\"modal-title\">${1:Modal title}</h4>\n\t</div>\n\t<div class=\"modal-body\">\n\t\t${2:<p>Modal body&hellip;</p>}\n\t</div>\n\t<div class=\"modal-footer\">\n\t\t<button type=\"button\" class=\"btn btn-secondary\" (click)=\"c('Close click')\">Close</button>\n\t</div>\n</template>\n<button class=\"btn btn-lg btn-outline-primary\" (click)=\"open(content)\">${3:Launch demo modal}</button>$4",
    "description": "NgBootstrap Modal"
  },
  "NgBootstrap Pagination": {
    "prefix": "ngb-pagination",
    "body": "<ngb-pagination [collectionSize]=\"${1:size}\" [(page)]=\"${2:page}\" [boundaryLinks]=\"${3:true}\" [directionLinks]=\"${4:true}\"></ngb-pagination>$5",
    "description": "NgBootstrap Pagination"
  },
  "NgBootstrap Popover": {
    "prefix": "ngb-popover",
    "body": "placement=\"${1:top|bottom|left|right}\" ngbPopover=\"${2:Popover content}\" popoverTitle=\"${3:Popover title}\"$4",
    "description": "NgBootstrap Popover"
  },
  "NgBootstrap Progress bar": {
    "prefix": "ngb-progressbar",
    "body": "<ngb-progressbar type=\"${1:success|info|warning|danger}\" [value]=\"${2:25}\" [max]=\"${3:50}\" [striped]=\"${4:true}\" [animated]=\"${5:true}\"></ngb-progressbar>$6",
    "description": "NgBootstrap Progress bar"    
  },
  "NgBootstrap Rating": {
    "prefix": "ngb-rating",
    "body": "<ngb-rating [(rate)]=\"${1:rate}\"></ngb-rating>$2",
    "description": "NgBootstrap Rating"    
  },
  "NgBootstrap Rating (ReadOnly)": {
    "prefix": "ngb-rating-readonly",
    "body": "<ngb-rating [(rate)]=\"${1:rate}\" [readonly]=\"${2:true}\"></ngb-rating>$3",
    "description": "NgBootstrap Rating"    
  },
  "NgBootstrap Tabset": {
    "prefix": "ngb-tabset",
    "body": "<ngb-tabset type=\"${1:tabs|pills}\">\n\t<ngb-tab title=\"${2:Title1}\">\n\t\t<template ngbTabContent>\n\t\t\t${3:<p>Content 1</p>}\n\t\t</template>\n\t</ngb-tab>\n\t<ngb-tab title=\"${4:Title2}\">\n\t\t<template ngbTabContent>\n\t\t\t${5:<p>Content 2</p>}\n\t\t</template>\n\t</ngb-tab>\n</ngb-tabset>$6",
    "description": "NgBootstrap Tabset"    
  },
  "NgBootstrap Tab": {
    "prefix": "ngb-tab",
    "body": "<ngb-tab title=\"${1:Title}\">\n\t<template ngbTabContent>\n\t\t${2:<p>Content</p>}\n\t</template>\n</ngb-tab>$3",
    "description": "NgBootstrap Tab"    
  },
  "NgBootstrap Tab (Custom title)": {
    "prefix": "ngb-tab",
    "body": "<ngb-tab>\n\t<template ngbTabTitle>\n\t\t${1:<i>Title</i>}\n\t</template>\n\t<template ngbTabContent>\n\t\t${2:<p>Content</p>}\n\t</template>\n</ngb-tab>$3",
    "description": "NgBootstrap Tab (Custom title)"    
  },
  "NgBootstrap Timepicker": {
    "prefix": "ngb-timepicker",
    "body": "<ngb-timepicker [(ngModel)]=\"${1:time}\" [seconds]=\"${2:true|false}\"></ngb-timepicker>$3",
    "description": "NgBootstrap Timepicker"    
  },
  "NgBootstrap Timepicker Meridian": {
    "prefix": "ngb-timepicker-meridian",
    "body": "<ngb-timepicker [(ngModel)]=\"${1:time}\" [seconds]=\"${2:true|false}\" [meridian]=\"${3:true|false}\"></ngb-timepicker>$4",
    "description": "NgBootstrap Timepicker Meridian"    
  },
  "NgBootstrap Timepicker Steps": {
    "prefix": "ngb-timepicker-steps",
    "body": "<ngb-timepicker [(ngModel)]=\"${1:time}\" [seconds]=\"${2:true|false}\" [meridian]=\"${3:true|false}\" [hourStep]=\"${4:1}\" [minuteStep]=\"${5:5}\" [secondStep]=\"${6:30}\"></ngb-timepicker>$7",
    "description": "NgBootstrap Timepicker Meridian"    
  },
  "NgBootstrap Tooltip": {
    "prefix": "ngb-tooltip",
    "body": "placement=\"${1:top|bottom|left|right}\" ngbTooltip=\"${2:Tooltip content}\"$4",
    "description": "NgBootstrap Tooltip"
  },
  "NgBootstrap Typeahead": {
    "prefix": "ngb-tooltip",
    "body": "<input type=\"text\" class=\"form-control\" [(ngModel)]=\"${1:model}\" [ngbTypeahead]=\"${2:search}\" />$4",
    "description": "NgBootstrap Typeahead"
  }
}